{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": "",
  "framework": null,
  "functions": {
    "api/errors/index.js": {
      "memory": 256,
      "maxDuration": 30
    },
    "api/health/index.js": {
      "memory": 128,
      "maxDuration": 5
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, X-API-Key" }
      ]
    }
  ]
}
